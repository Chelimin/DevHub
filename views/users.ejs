<script src='Chart.min.js'></script>
<h4>Developers</h4>
<div class="row">

  <% users.forEach( user => { %>
    <div class="col s12 m4 l3">
      <div class="card s">
        <div class="card-top">
          <span><h5><%= user.github.username %></h4></span>
          <div class="spacer">
            <br>
            <br>
          </div>
        <div class="card-sizer valign-wrapper">
           <div class="card-image waves-effect waves-block waves-light valign">
             <img class="activator developer" src=" https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcSG6dQDBnjtwKjpzKscj13tq71esfjcT-LqCLibMz8jTuSpizTEJRhgDA">
           </div>
        </div>

      </div>

      <div class="card-content">
        <span class="card-title activator grey-text text-darken-4"><%= user.github.username %><i class="material-icons right">more_vert</i></span>
        <p>
        <strong> Name: </strong> <%= user.github.username %>
        <br>

        <br>
        <strong> Rockstar: </strong> <%= user.data.rockStar %>
        <strong> Longest Streak: </strong> <%= user.data.longestStreak %>
        </p>
      </div>

      <div class="card-reveal">
        <span class="card-title grey-text text-darken-4"><%= user.github.username %><i class="material-icons right">close</i></span>
        <ul>
          <li>
            <strong> Stack:
              <% for (var z in user.data.languages) %>
              <i class="devicon-<%= z.toLowerCase() %>-plain colored size"></i>
            </strong>
          </li>
          <li>
             <strong> Education:
             </strong>
           </li>
           <li>
             <strong> Work Experience:
             </strong>
           </li>
        </ul>
      </div>
    </div>
    <% }) %>
  </div>
  <br>
</div>

